<?xml version='1.0' encoding='utf-8'?>
<widget id="com.frog.cordova" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <name>CordovaDemo</name>
    <description>
        A simple Apache Cordova application designed to test multiple interaction models.
    </description>
    <author email="karl.carstensen@gmail.com" href="http://www.karlcarstensen.com">
        Karl Carstensen
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="EnableViewportScale" value="true" />
</widget>
